{
  "version": "4.3",
  "pipeline_parameter_map": {
    "faster": {
      "common": ["common_transcriber_options", "common_decoder_options"],
      "engine": ["faster_whisper_engine_options", "stable_ts_engine_options"],
      "vad": "stable_ts_vad_options",
      "vad_handling": "packed"
    },
    "fast": {
      "common": ["common_transcriber_options", "common_decoder_options"],
      "engine": ["openai_whisper_engine_options", "stable_ts_engine_options"],
      "vad": "stable_ts_vad_options",
      "vad_handling": "packed"
    },
    "balanced": {
      "common": ["common_transcriber_options", "common_decoder_options", "exclusive_whisper_plus_faster_whisper"],
      "engine": "openai_whisper_engine_options",
      "vad": "silero_vad_options",
      "vad_handling": "separate"
    }
  },
  "models": {
    "whisper-turbo": {
      "provider": "openai_whisper",
      "model_name": "turbo",
      "device": "cuda",
      "compute_type": "float16",
      "supported_tasks": [
        "transcribe"
      ]
    },
    "whisper-large-v2": {
      "provider": "openai_whisper",
      "model_name": "large-v2",
      "device": "cuda",
      "compute_type": "float16",
      "supported_tasks": [
        "transcribe",
        "translate"
      ]
    }
  },
  "vad_engines": {
    "silero-v4": {
      "provider": "silero",
      "repo": "snakers4/silero-vad:v4.0"
    },
    "silero-v3.1": {
      "provider": "silero",
      "repo": "snakers4/silero-vad:v3.1"
    },
    "silero-v5.1": {
      "provider": "silero",
      "repo": "snakers4/silero-vad"
    }
  },
  "feature_configs": {
    "scene_detection": {
      "default": {
        "max_duration_s": 29.0,
        "min_duration_s": 0.30,

        "target_sr": 16000,
        "force_mono": true,
        "preserve_original_sr": true,

        "assist_processing": false,
        "verbose_summary": true,

        "pass1_min_duration_s": 0.30,
        "pass1_max_duration_s": 2700.0,
        "pass1_max_silence_s": 1.80,
        "pass1_energy_threshold": 38,

        "pass2_min_duration_s": 0.30,
        "pass2_max_duration_s": 28.0, 
        "pass2_max_silence_s": 0.94,
        "pass2_energy_threshold": 50,

        "bandpass_low_hz": 200,
        "bandpass_high_hz": 4000,
        "drc_threshold_db": -24.0,
        "drc_ratio": 4.0,
        "drc_attack_ms": 5.0,
        "drc_release_ms": 100.0,
        "skip_assist_on_loud_dbfs": -5.0,

        "brute_force_fallback": true,
        "brute_force_chunk_s": 29.0,
        "pad_edges_s": 0.0,
        "fade_ms": 0
      }
    },
    "post_processing": {
      "standard": {
        "remove_hallucinations": true,
        "max_merge_gap_sec": 0.4
      }
    }
  },
  "pipelines": {
    "faster": {
      "workflow": {
        "model": "whisper-turbo",
        "vad": "none",
        "backend": "stable-ts",
        "features": {
          "post_processing": "standard"
        }
      },
      "model_overrides": {
        "translate": "whisper-large-v2"
      }
    },
    "fast": {
      "workflow": {
        "model": "whisper-turbo",
        "vad": "none",
        "backend": "stable-ts",
        "features": {
          "scene_detection": "default",
          "post_processing": "standard"
        }
      }
    },
    "balanced": {
      "workflow": {
        "model": "whisper-turbo",
        "vad": "silero-v4",
        "backend": "whisper",
        "features": {
          "scene_detection": "default",
          "post_processing": "standard"
        }
      }
    }
  },
  "defaults": {
    "language": "ja",
    "output_format": "srt"
  },
  "ui_preferences": {
    "console_verbosity": "quiet",
    "progress_batch_size": 10,
    "show_scene_details": false,
    "max_console_lines": 1000,
    "auto_scroll": true,
    "show_timestamps": false,
    "theme": "default"
  },
  "common_transcriber_options": {
    "balanced": {
      "temperature": [
        0.0,
        0.1
      ],
      "compression_ratio_threshold": 2.4,
      "logprob_threshold": -1.0,
      "no_speech_threshold": 0.6,
      "condition_on_previous_text": false,
      "initial_prompt": null,
      "word_timestamps": true,
      "prepend_punctuations": null,
      "append_punctuations": null,
      "clip_timestamps": null
    },
    "conservative": {
      "temperature": [
        0.0
      ],
      "compression_ratio_threshold": 2.4,
      "logprob_threshold": -1.0,
      "no_speech_threshold": 0.74,
      "condition_on_previous_text": false,
      "initial_prompt": null,
      "word_timestamps": true,
      "prepend_punctuations": null,
      "append_punctuations": null,
      "clip_timestamps": null
    },
    "aggressive": {
      "temperature": [
        0.0,
