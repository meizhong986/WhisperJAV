{
  "version": "4.4",
  "pipeline_parameter_map": {
    "faster": {
      "common": [
        "common_transcriber_options",
        "common_decoder_options"
      ],
      "engine": [
        "faster_whisper_engine_options",
        "stable_ts_engine_options"
      ],
      "vad": "stable_ts_vad_options",
      "vad_handling": "packed"
    },
    "fast": {
      "common": [
        "common_transcriber_options",
        "common_decoder_options"
      ],
      "engine": [
        "faster_whisper_engine_options",
        "fast_pipeline_overrides",
        "stable_ts_engine_options"
      ],
      "vad": "stable_ts_vad_options",
      "vad_handling": "packed"
    },
    "fidelity": {
      "common": [
        "common_transcriber_options",
        "common_decoder_options",
        "exclusive_whisper_plus_faster_whisper"
      ],
      "engine": "openai_whisper_engine_options",
      "vad": "silero_vad_options",
      "vad_handling": "separate"
    },
    "balanced": {
      "common": [
        "common_transcriber_options",
        "common_decoder_options",
        "exclusive_whisper_plus_faster_whisper"
      ],
      "engine": "faster_whisper_engine_options",
      "vad": "silero_vad_options",
      "vad_handling": "separate"
    }
  },
  "fast_pipeline_overrides": {
    "balanced": {
      "batch_size": 1,
      "chunk_length": null
    },
    "conservative": {
      "batch_size": 1,
      "chunk_length": null
    },
    "aggressive": {
      "batch_size": 1,
      "chunk_length": null
    }
  },
  "models": {
    "whisper-turbo": {
      "provider": "openai_whisper",
      "model_name": "turbo",
      "compute_type": "float16",
      "supported_tasks": [
        "transcribe"
      ],
      "_comment": "device auto-detected: CUDA → MPS → CPU"
    },
    "whisper-large-v2": {
      "provider": "openai_whisper",
      "model_name": "large-v2",
      "compute_type": "float16",
      "supported_tasks": [
        "transcribe",
        "translate"
      ],
      "_comment": "device auto-detected: CUDA → MPS → CPU"
    },
    "faster-whisper-large-v2-int8": {
      "provider": "faster_whisper",
      "model_name": "large-v2",
      "compute_type": "int8_float16",
      "supported_tasks": [
        "transcribe",
        "translate"
      ],
      "_comment": "requires ctranslate2 build with int8 quantization support"
    },
    "faster-whisper-large-v3": {
      "provider": "faster_whisper",
      "model_name": "large-v3",
      "compute_type": "float16",
      "supported_tasks": [
        "transcribe",
        "translate"
      ],
      "_comment": "Systran/faster-whisper-large-v3 for fast/faster pipelines"
    }
  },
  "vad_engines": {
    "silero-v4": {
      "provider": "silero",
      "repo": "snakers4/silero-vad:v4.0"
    },
    "silero-v3.1": {
      "provider": "silero",
      "repo": "snakers4/silero-vad:v3.1"
    },
    "silero-latest": {
      "provider": "silero",
      "repo": "snakers4/silero-vad"
    }
  },
  "feature_configs": {
    "scene_detection": {
      "default_method": "auditok",
      "auditok": {
        "max_duration_s": 29.0,
        "min_duration_s": 0.2,
        "target_sr": 16000,
        "force_mono": true,
        "preserve_original_sr": true,
        "assist_processing": false,
        "verbose_summary": true,
        "pass1_min_duration_s": 2.0,
        "pass1_max_duration_s": 2700.0,
        "pass1_max_silence_s": 2.5,
        "pass1_energy_threshold": 32,
        "pass2_min_duration_s": 0.1,
        "pass2_max_duration_s": 1800.0,
        "pass2_max_silence_s": 1.8,
        "pass2_energy_threshold": 38,
        "bandpass_low_hz": 200,
        "bandpass_high_hz": 4000,
        "drc_threshold_db": -24.0,
        "drc_ratio": 4.0,
        "drc_attack_ms": 5.0,
        "drc_release_ms": 100.0,
        "skip_assist_on_loud_dbfs": -5.0,
        "brute_force_fallback": true,
        "brute_force_chunk_s": 29.0,
        "pad_edges_s": 0.0,
        "fade_ms": 0
      },
      "silero": {
        "method": "silero",
        "max_duration_s": 29.0,
        "min_duration_s": 0.2,
        "target_sr": 16000,
        "force_mono": true,
        "preserve_original_sr": true,
        "assist_processing": false,
        "verbose_summary": true,
        "pass1_min_duration_s": 2.0,
        "pass1_max_duration_s": 2700.0,
        "pass1_max_silence_s": 2.5,
        "pass1_energy_threshold": 32,
        "pass2_min_duration_s": 0.1,
        "pass2_max_duration_s": 960.0,
        "silero_threshold": 0.08,
        "silero_neg_threshold": 0.1,
        "silero_min_silence_ms": 7800,
        "silero_min_speech_ms": 100,
        "silero_max_speech_s": 960,
        "silero_min_silence_at_max": 750,
        "silero_speech_pad_ms": 500,
        "bandpass_low_hz": 200,
        "bandpass_high_hz": 4000,
        "drc_threshold_db": -24.0,
        "drc_ratio": 4.0,
        "drc_attack_ms": 5.0,
        "drc_release_ms": 100.0,
        "skip_assist_on_loud_dbfs": -5.0,
        "brute_force_fallback": true,
        "brute_force_chunk_s": 29.0,
        "pad_edges_s": 0.0,
        "fade_ms": 0
      }
    },
    "post_processing": {
      "standard": {
        "remove_hallucinations": true,
        "max_merge_gap_sec": 0.4
      }
    }
  },
  "pipelines": {
    "faster": {
      "workflow": {
        "model": "faster-whisper-large-v3",
        "vad": "none",
        "backend": "stable-ts",
        "features": {
          "post_processing": "standard"
        }
      },
      "model_overrides": {
        "translate": "whisper-large-v2"
      }
    },
    "fast": {
      "workflow": {
        "model": "faster-whisper-large-v2-int8",
        "vad": "none",
        "backend": "stable-ts",
        "features": {
          "scene_detection": "default",
          "post_processing": "standard"
        }
      }
    },
    "fidelity": {
      "workflow": {
        "model": "whisper-large-v2",
        "vad": "silero-v3.1",
        "backend": "whisper",
        "features": {
          "scene_detection": "default",
          "post_processing": "standard"
        }
      }
    },
    "balanced": {
      "workflow": {
        "model": "faster-whisper-large-v2-int8",
        "vad": "silero-v3.1",
        "backend": "faster-whisper",
        "features": {
          "scene_detection": "default",
          "post_processing": "standard"
        }
      },
      "model_overrides": {
        "translate": "faster-whisper-large-v2-int8"
      }
    }
  },
  "defaults": {
    "language": "ja",
    "output_format": "srt"
  },
  "ui_preferences": {
    "console_verbosity": "summary",
    "progress_batch_size": 10,
    "show_scene_details": false,
    "max_console_lines": 1000,
    "auto_scroll": true,
    "show_timestamps": false,
    "theme": "default",
    "last_mode": "fidelity",
